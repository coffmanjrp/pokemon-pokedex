{
  "name": "pokemon-pokedex-server",
  "version": "1.0.0",
  "description": "GraphQL server for Pokemon Pokedex application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "railway:build": "npm run build",
    "railway:start": "npm run start",
    "sync:all": "ts-node src/sync/syncRunner.ts all",
    "sync:pokemon": "ts-node src/sync/syncRunner.ts pokemon",
    "sync:gen": "ts-node src/sync/syncRunner.ts gen",
    "sync:forms": "ts-node src/sync/syncRunner.ts forms",
    "sync:test": "ts-node src/sync/syncRunner.ts test",
    "sync:evolution": "ts-node src/sync/syncEvolutionChains.ts",
    "sync:evolution:enriched": "ts-node src/sync/syncEnrichedEvolutions.ts",
    "sync:test-single": "ts-node src/sync/testSinglePokemonSync.ts",
    "sync:debug": "ts-node src/sync/debug.ts",
    "sync:check-evolution": "ts-node src/sync/checkEvolutionChains.ts",
    "sync:test-evolution": "ts-node src/sync/testEvolutionQuery.ts",
    "sync:test-enriched": "ts-node src/sync/testEnrichedEvolutionQuery.ts",
    "sync:test-details": "ts-node src/sync/testEvolutionDetails.ts",
    "sync:count-evolution": "ts-node src/sync/countEvolutionChains.ts",
    "sync:list-evolution": "ts-node src/sync/listAllEvolutionChains.ts",
    "sync:check-count": "ts-node src/sync/checkPokemonCount.ts",
    "sync:find-all-chains": "ts-node src/sync/findAllEvolutionChainIds.ts",
    "sync:evolution:gen2plus": "ts-node src/sync/syncGen2PlusEvolutions.ts",
    "sync:test-gen2": "ts-node src/sync/testGen2Evolution.ts",
    "sync:check-synced": "ts-node src/sync/checkSyncedChains.ts"
  },
  "keywords": [
    "pokemon",
    "graphql",
    "apollo-server",
    "typescript",
    "express"
  ],
  "author": "Paul Coffman Jr",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@supabase/supabase-js": "^2.52.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
