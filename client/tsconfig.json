{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "exactOptionalPropertyTypes": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next-gen-1/types/**/*.ts",
    ".next-gen-2/types/**/*.ts",
    ".next-gen-3/types/**/*.ts",
    ".next-gen-4/types/**/*.ts",
    ".next-gen-5/types/**/*.ts",
    ".next-gen-6/types/**/*.ts",
    ".next-gen-7/types/**/*.ts",
    ".next-gen-8/types/**/*.ts",
    ".next/types/**/*.ts",
    "jest.setup.js",
    "next-env.d.ts",
    ".next-gen-9/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
